{
  "$id": "wpSchema/v1.0",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cardPrintDate": {
      "type": "string",
      "format": "date-time"
    },
    "passType": {
      "description": "The date that this certificate was issued by the issuer(s)",
      "type": "string",
      "enum": ["Employment Pass"]
    },
    "passStatus": {
      "type": "string",
      "enum": ["Live"]
    },

    "expiryDate": {
      "type": "string",
      "format": "date-time"
    },
    "legalTill": {
      "type": "string",
      "format": "date-time"
    },
    "applicationDate": {
      "type": "string",
      "format": "date-time"
    },
    "issueDate": {
      "type": "string",
      "format": "date-time"
    },
    "renewalDate": {
      "type": "string",
      "format": "date-time"
    },
    "isMultipleJourney": {
      "type": "boolean"
    },
    "serial": {
      "type": "string"
    },
    "recipient": {
      "type": "object",
      "properties": {
        "fin": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "occupation": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "dob": {
          "type": "string",
          "format": "date-time"
        },
        "maritalStatus": {
          "type": "string",
          "enum": ["married", "single", "divorced"]
        }
      }
    },
    "employer": {
      "type": "object",
      "properties": {
        "uen": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "sector": {
          "type": "string"
        }
      }
    }
  },
  "additionalProperties": false
}
